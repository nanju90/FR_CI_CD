pipeline {
	agent any
	stages {
		stage ('Unit Test') {
			steps{
				echo "Unit Test Stage"
			Checkpoint "Unit Test Complete"
			}
		}
		stage ('Build') {
			steps{	
				sh """
				mvn -version
				"""
				Checkpoint "Unit Test Complete"
				}
		}
		stage ('Publish to Nexus') {
			steps{
				echo "Publlish stage"
				Checkpoint "Unit Test Complete"
			}
		}
		stage ('Deploy to DEV/QA') {
			steps{
				echo "Deploying to Dev/Qa"
				Checkpoint "Unit Test Complete"
			}
		}
		stage ('Release to Nexus') {
			steps{
				echo "Publishing to release repo"
				Checkpoint "Unit Test Complete"
			}
		}
		
	}
}
