pipeline {
	agent any
	stages {
		stage ('Unit Test') {
			steps{
				echo "Unit Test Stage"
			}
		}
		stage ('Build') {
			steps{	
				echo "Building..."
				 withMaven(maven : 'apache-maven-3.6.1')
                		 bat'mvn clean compile'
			}
		}
		stage ('Publish to Nexus') {
			steps{
				echo "Publlish stage"
			}
		}
		stage ('Deploy to DEV/QA') {
			steps{
				echo "Deploying to Dev/Qa"
			}
		}
		stage ('Release to Nexus') {
			steps{
				echo "Publishing to release repo"
			}
		}
		
	}
}
